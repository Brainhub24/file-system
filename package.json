{
    "name": "lowlight-file-system",
    "description": "This library is an implementation of an \"homemade\" File System, similar to FAT. It's based on steganography so data can be stored and loaded from a supported image file. A GUI is also included with multiples applications.",
    "version": "1.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/lowlighter/file-system"
    },
    "author": "lowlight(er)",
    "license": "MIT",
    "dependencies": {
        "babel-minify": "^0.2.0",
        "jsdoc": "^3.4.3",
        "less": "^2.7.3",
        "less-plugin-clean-css": "^1.5.1",
        "marked": "^0.3.9"
    },
    "scripts": {
        "build": "node ./src/build.js"
    },
    "config": {
        "project_name": "File System",
        "project_repo": "https://github.com/lowlighter/file-system",
        "project_docs": "https://lowlighter.github.io/file-system/docs",
        "project_page": "https://lowlight.fr/en/blog/file-system/",
        "source": "./lowlight.fs.js",
        "output": "./../bin/lowlight.fs.js",
        "jsdoc_source": "../../../bin/lowlight.fs.js",
        "jsdoc_output": "../../../docs",
        "jsdoc_config": "./../../conf.json",
        "jsdoc_readme": "../../../README.md",
        "demo_template": "./demo/template.html",
        "demo_output": "./demo/index.html"
    }
}
